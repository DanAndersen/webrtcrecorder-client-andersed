{
  /* The ID of the node instance */
  "nodeId" : 1,

  /* The port to listen on */
  /*"port" : 4500, */
  "port" : 443,

  /* Allow anonymous connections */
  "anonymous" : true,

  /* Session ttl in minutes */
  "sessionTtl" : 15,/*,*/

  /* Redis configuration
  "redis" : {
    "host" : "localhost",
    "port" : 6379
  }, */

  /* SSL configuration */
  
  "ssl" : {
    "enabled" : true,
    "key" : "certs/privkey1.pem",
    "cert" : "certs/fullchain1.pem"
  }
  
}
